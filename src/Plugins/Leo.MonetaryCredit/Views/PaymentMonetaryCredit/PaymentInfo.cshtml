@model Leo.MonetaryCredit.Models.PaymentInfoModel
@{
    Layout = "";
}
<div class="container">
    <div class="row align-items-center mb-3">
        <div class="col-md-4 col-12 text-md-right">
            <label class="col-form-label" asp-for="YuE">@Loc["Payment.YuE"]:</label>
        </div>
        <div class="col-md-4 col-12 text-right">
            <input asp-for="YuE" class="form-control" autofocus="autofocus" asp-disabled="true" />
        </div>
    </div>
</div>
@if (!string.IsNullOrEmpty(Model.Errors))
{
    <div class="text-danger">@Model.Errors</div>
}